  "main": "index.js",
  "bin": {
    "tsheets": "./index.js"
  },
  "scripts": {
    "test": "jest",
    "release": "release-it"
  },
  "dependencies": {
    "babel-jest": "^26.6.3",
    "chalk": "^4.1.0",
    "configstore": "^5.0.1",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "inquirer": "^8.0.0",
    "open": "^8.0.2",
    "request": "^2.88.2",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.0",
    "jest": "^26.6.3",
